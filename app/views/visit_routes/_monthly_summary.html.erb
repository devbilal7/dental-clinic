                                <!-- Dentist -->
                                <% @dentists.each do |dentist|  %>
                                    <% dentist_data("dentist",@clinic_id,dentist.id) %>
                                    <%= render 'monthly_visit_route', consultant: dentist %>
                                <% end %>                  
                                    
                                <!-- DentistHygienist -->
                                <% @dentist_hygienists.each do |dentist_hygienist|  %>
                                    <% dentist_data("dentist_hygienist",@clinic_id,dentist_hygienist.id) %>
                                    <%= render 'monthly_visit_route', consultant: dentist_hygienist %>
                                <% end %> 

                                <!-- Treatment Coordinator --> 
                                <% @treatment_coordinators.each do |treatment_coordinator|  %>
                                    <% dentist_data("treatment_coordinator",@clinic_id,treatment_coordinator.id) %>
                                    <%= render 'monthly_visit_route', consultant: treatment_coordinator %>
